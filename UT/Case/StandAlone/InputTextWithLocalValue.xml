<testCase>
	<title>Input Text With Local Value</title>
	<description>Input Text With Local Value</description>
	<stopIfFailed>false</stopIfFailed>
	<repeat>1</repeat>
	<configFile>local.properties</configFile>
	
	<action>
		<name>openWebPage</name>
		<value>${url}</value>
	</action>
	<action>
		<name>switchFrame</name>
		<value>myDetail</value>
	</action>
	<action>
		<name>inputText</name>
		<element>myInputText</element>
		<elementBy>id</elementBy>
		<value>${input_value}</value>
	</action>
	<action>
		<name>closeBrowser</name>
	</action>
</testCase>