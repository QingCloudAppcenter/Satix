<testCase>
	<title>Wait Page To Load</title>
	<description>Wait Page To Load</description>
	<stopIfFailed>false</stopIfFailed>
	<repeat>1</repeat>
	
	<action>
		<name>openWebPage</name>
		<value>${url}</value>
	</action>
	<action>
		<name>waitPageToLoad</name>
		<elapse>2000</elapse>
	</action>
	<action>
		<name>closeBrowser</name>
	</action>
</testCase>